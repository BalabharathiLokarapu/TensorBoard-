{
  "compilerOptions": {
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": ["dom", "es2020"],
    "moduleResolution": "node",
    "noResolve": true,
    "preserveConstEnums": false,
    "sourceMap": false,
    "target": "es5",
    "useUnknownInCatchVariables": true,
  }
}
