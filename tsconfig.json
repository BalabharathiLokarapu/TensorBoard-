{
  "compilerOptions": {
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "lib": ["dom", "es2018"],
    "module": "ES6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  },
  "exclude": ["bazel-*"]
}
